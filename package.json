{
  "name": "cartransport-au",
  "type": "module",
  "version": "0.0.1",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,html}": [
      "prettier --write"
    ]
  },
  "scripts": {
